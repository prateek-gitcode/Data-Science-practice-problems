library(readxl)
bank= read.csv('bank-full.csv')
new_bank<- subset(bank,select = c(age,balance,duration,y))
new_bank$y<- factor(new_bank$y,
levels = c('no','yes'),
labels = c(0,1))
attach(new_bank)
library(caTools)
set.seed(123)
split= sample.split(new_bank$y,SplitRatio = 0.8)
train_set= subset(new_bank,split==TRUE)
test_set= subset(new_bank,split==FALSE)
train_set[,1:3]= scale(train_set[,1:3])
test_set[,1:3]= scale(test_set[,1:3])
classifier= glm(formula = y ~ .,family = binomial,data = train_set)
prob_pred= predict(classifier,type = 'response',newdata = test_set[-4])
prob_pred
y_pred= ifelse(prob_pred>0.5,1,0)
y_pred
View(test_set)
